
{
  "manifest_version": 2,
  "name": "Window State Sample",
  "version": "4.2",
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "permissions": [
    "webview",
    "fullscreen",
    "alwaysOnTopWindows",
    "browser"],
    "externally_connectable": {
      "matches": ["*://p-p.me/*"]
    }
}
